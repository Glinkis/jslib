{
  "compilerOptions": {
    "target": "ES2015",
    "module": "Preserve",
    "lib": ["es2015", "es2016", "es2017", "dom",],
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noEmit": true,
  },
  "include": ["src", "rollup.config.ts"],
  "exclude": ["node_modules"]
}
